networks:
  default:
    external: true
    name: weather
services:
  db:
    container_name: SqlServer
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 123456a@
    image: mcr.microsoft.com/mssql/server
    networks:
      default: {}
    ports:
    - published: 1433
      target: 1433
    volumes:
    - Sql-server-storage:/var/opt/mssql:rw
  weatherwebapi:
    build:
      context: C:\Users\torbjorn.jacobsen\source\repos\WeatherSummary\WeatherWebAPI\WeatherWebAPI
      dockerfile: WeatherWebAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: weatherwebapi
    networks:
      default: {}
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\torbjorn.jacobsen\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\torbjorn.jacobsen\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'
volumes:
  Sql-server-storage:
    external: true
    name: Sql-server-storage