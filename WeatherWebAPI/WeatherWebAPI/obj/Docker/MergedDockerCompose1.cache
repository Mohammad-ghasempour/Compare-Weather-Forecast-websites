networks:
  default:
    external: true
    name: weather
services:
  db:
    container_name: SqlServer
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 123456a@
    image: mcr.microsoft.com/mssql/server
    networks:
      default: {}
    ports:
    - published: 1433
      target: 1433
    volumes:
    - Sql-server-storage:/var/opt/mssql:rw
  weatherwebapi:
    build:
      context: C:\Users\torbjorn.jacobsen\source\repos\WeatherSummary\WeatherWebAPI\WeatherWebAPI
      dockerfile: WeatherWebAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WeatherWebAPI
      target: base
    container_name: WeatherWebAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: weatherwebapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/WeatherWebAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: {}
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\torbjorn.jacobsen\source\repos\WeatherSummary\WeatherWebAPI\WeatherWebAPI\WeatherWebAPI:/app:rw
    - C:\Users\torbjorn.jacobsen\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\torbjorn.jacobsen\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\torbjorn.jacobsen\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\torbjorn.jacobsen\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\torbjorn.jacobsen\source\repos\WeatherSummary\WeatherWebAPI\WeatherWebAPI:/src:rw
version: '3.4'
volumes:
  Sql-server-storage:
    external: true
    name: Sql-server-storage